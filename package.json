{
  "name": "entsoe-api-node",
  "version": "1.2.0",
  "description": "Unofficial API wrapper for the ENTSO-E Transparency Platform API (https://transparency.entsoe.eu)",
  "author": "Robin Hansson <holla@rabinage.dev> (https://github.com/rabinage)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rabinage/entsoe-api-node.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "entso-e"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx swc src --delete-dir-on-start --out-dir dist --strip-leading-paths",
    "example:dev": "nodemon --watch example",
    "test": "node -r \"dotenv/config\" test/http-client.test.mjs",
    "format": "npx prettier --write . && npx eslint --fix src test example",
    "lint": "npx prettier --check . && npx eslint --max-warnings=0 .",
    "precommit": "npm run lint"
  },
  "engines": {
    "node": ">=14"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "node -r \"dotenv/config\" -r \"@swc/register\""
    },
    "quiet": true
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.21",
    "@swc/register": "0.1.10",
    "@types/node": "22.14.1",
    "dotenv": "16.5.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-prettier": "5.2.6",
    "husky": "9.1.7",
    "nodemon": "3.1.9",
    "prettier": "3.5.3"
  },
  "dependencies": {
    "undici": "7.8.0",
    "xml2js": "0.6.2"
  },
  "overrides": {
    "json5@1.0.1": "1.0.2"
  }
}
