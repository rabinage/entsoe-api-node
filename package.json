{
  "name": "entsoe-api-node",
  "version": "1.1.0",
  "description": "Unofficial API wrapper for the ENTSO-E Transparency Platform API (https://transparency.entsoe.eu)",
  "author": "Robin Hansson <holla@rabinage.dev> (https://github.com/rabinage)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rabinage/entsoe-api-node.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "entso-e"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js"  
    }
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx swc src --delete-dir-on-start --out-dir dist --strip-leading-paths",
    "example:dev": "nodemon --watch example",
    "test": "node -r \"dotenv/config\" test/http-client.test.mjs",
    "format": "npx prettier --write . && npx eslint --fix src test example",
    "lint": "npx prettier --check . && npx eslint --max-warnings=0 src test example",
    "precommit": "npm run lint"
  },
  "engines": {
    "node": ">=16"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "node -r \"dotenv/config\" -r \"@swc/register\""
    },
    "quiet": true
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.40",
    "@swc/register": "0.1.10",
    "@types/node": "20.17.3",
    "dotenv": "16.4.5",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.11.1",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.6",
    "nodemon": "3.1.7",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "undici": "6.20.1",
    "xml2js": "0.6.2"
  },
  "overrides": {
    "json5@1.0.1": "1.0.2"
  }
}
